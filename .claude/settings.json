{
  "env": {
    "addintolocalGH_TOKEN": "ghp_xABCD",
    "GH_USERNAME": "CodespaceAgent:Claude-Do",
    "GH_EMAIL": "claude@claude.do",
    "addintolocalNTFY_CHANNEL": "abc1234"
  },
  "permissions": {
    "allow": [
      "WebSearch",
      "Bash(find:*)",
      "Bash(gh issue list:*)",
      "Bash(gh issue view:*)",
      "Bash(gh pr comment:*)",
      "Bash(gh pr list:*)",
      "Bash(gh release view:*)",
      "Bash(git fetch:*)"
    ]
  },
  "hooks": {
    "SessionStart": [
      {
        "matcher": "startup",
        "hooks": [
          {
            "type": "command",
            "command": "$CLAUDE_PROJECT_DIR/.claude/hooks/setup-cc-web.sh",
            "timeout": 5000
          }
        ]
      },
      {
        "matcher": "resume",
        "hooks": [
          {
            "type": "command",
            "command": "true || echo 'Resumed session hook executed.'"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "frontend-design@claude-plugins-official": true,
    "pr-review-toolkit@claude-plugins-official": true,
    "code-review@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true,
    "rust-analyzer-lsp@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true
  }
}
